#include <iostream>  
using namespace std;  

int main() {  

  int numero,s1=0,s2=1,s3,i;   

 cout<<"insira o termo desejado da sequencia: ";    
 cin>>numero;    
 cout<<s1<<" "<<s2<<" "; //printing 0 and 1    
 for(i=2;i<numero;++i) //loop starts from 2 because 0 and 1 are already printed    
 {    
  s3=s1+s2;    
  cout<<s3<<" ";    
  s1=s2;    
  s2=s3;    
 }   
}  
